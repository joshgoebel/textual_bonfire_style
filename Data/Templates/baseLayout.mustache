        <div class="inline_nickname" colornumber="18">SomeLongNickname 18</div>
        <div class="inline_nickname" colornumber="19">SomeLongNickname 19</div>
        <div class="inline_nickname" colornumber="20">SomeLongNickname 20</div>
        <div class="inline_nickname" colornumber="21">SomeLongNickname 21</div>
        <div class="inline_nickname" colornumber="22">SomeLongNickname 22</div>
        <div class="inline_nickname" colornumber="23">SomeLongNickname 23</div>
        <div class="inline_nickname" colornumber="24">SomeLongNickname 24</div>
        <div class="inline_nickname" colornumber="25">SomeLongNickname 25</div>
        <div class="inline_nickname" colornumber="26">SomeLongNickname 26</div>
