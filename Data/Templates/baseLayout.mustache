        <div class="inline_nickname" colornumber="29">SomeLongNickname 29</div>
        <div class="inline_nickname" colornumber="30">SomeLongNickname 30</div>
      </div>

			<div id="hello">
        <div>
        <img src="{{applicationResourcePath}}/applicationIconYosemite.icns" 
        width="128" height="128" id="appicon" />
            
        <span class="channel">{{channelName}}
        {{^isChannelView}}{{configuredServerName}}{{/isChannelView}}
      </span>
			  <p>
			    {{#isChannelView}}
			    You have not joined this channel yet.
			    {{/isChannelView}}
			    {{^isChannelView}}
			    You have not connected to this server yet.
			    {{/isChannelView}}

		    </p>
        </div>
			</div>
      <div id="status" style="display:none">
        <p></p>
      </div>

			{{#isChannelView}}
				<div id="topic_bar" ondblclick="Textual.topicDoubleClicked()">{{{formattedTopicValue}}}</div>
			{{/isChannelView}}
	</body>
</html>